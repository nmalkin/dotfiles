[user]
	email =
	name =
[core]
	excludesfile = ~/.gitignore_global
[alias]
  br = branch
  ca = commit -a
  cb = checkout -b
  ci = commit
  co = checkout
  cp = cherry-pick
  i  = diff
  iw = diff -w
  lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
  m  = checkout master
  pl = pull
  st = status
  statu = status
[push]
	default = simple
[pull]
	ff = only
